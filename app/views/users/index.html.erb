<% provide(:title, 'All users') %>
<div class="row">
<aside class = "col-md-6">

    <section class = "ut">

        <%= will_paginate %>

        <h1>Users</h1>

        <%= search_form_for @q do |f| %>
        <p>
          <%= f.search_field :name_or_nickname_cont, placeholder: "Search users" %>

          <%= f.submit "Search", id:'bottone' %>
        </p>
        <% end %>


        <ul class="users">
                <%= render @users %>
        </ul>

       </section>

</aside>
<div class = "col-md-6">

        <%= will_paginate %>

        <h1>Travels</h1>
        <%= form_tag users_path, :method => :get do %>
          <p>
            <%= text_field_tag :search, params[:search], placeholder: "Search localities"%>
            <%= submit_tag "Search Near", :name => nil, id:"bottone" %>
          </p>
        <% end %>


        <ul class="travels">
                <%= render @travels %>
        </ul>
    </div>


</div>



