<li>
    <%= gravatar_for user, size: 50 %>
    <%= link_to user.name, user, id:"link_barra" %> <br>
    <%= user.nickname %>
    <% if user_signed_in? %>
        <% if current_user.admin? && current_user!=user %>
        | <%= link_to "delete", user, method: :delete,
                                      data: { confirm: "You sure?" }, id:"link_barra" %>
        <% end %>
    <%end%>
</li>
